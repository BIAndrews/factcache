{
  "name": "bryana-factcache",
  "version": "0.1.0",
  "author": "Bryan Andrews",
  "summary": "Factcache caches facter facts locally for 3rd party offline integration",
  "license": "GPLv3",
  "source": "https://github.com/BIAndrews/factcache",
  "project_page": "https://github.com/BIAndrews/factcache",
  "issues_url": "https://github.com/BIAndrews/factcache/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
